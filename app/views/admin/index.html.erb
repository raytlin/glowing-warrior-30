<h1>Admin Login </h1>
<%= error_messages_for :admin_user %>


	
<% form_for :admin_user, @admin_user, :url => { :action => "send_login" } do |f| %>
  <p>
    <b>Username</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Password</b><br />
    <%= f.password_field :password %>
  </p>
  
  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>
