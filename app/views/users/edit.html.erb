<h1>Editing user</h1>

<%= error_messages_for :user %>

<% form_for(@user) do |f| %>
  <p>
    <b>Email</b><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <b>Password</b><br />
    <%= f.text_field :password %>
  </p>
  
   <p>
    <b>First Name</b><br />
    <%= f.text_field :first_name %>
  </p>
  
   <p>
    <b>Last Name</b><br />
    <%= f.text_field :last_name %>
  </p>
<p>
	<b>Image Path</b> <br />
	<%= f.text_field :image_path %>
	
	


  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @user %> |
<%= link_to 'Back', users_path %>
