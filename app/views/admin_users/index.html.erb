<h1>Listing admin_users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Password</th>
  </tr>

<% for admin_user in @admin_users %>
  <tr>
    <td><%=h admin_user.name %></td>
    <td><%=h admin_user.password %></td>
    <td><%= link_to 'Show', admin_user %></td>
    <td><%= link_to 'Edit', edit_admin_user_path(admin_user) %></td>
    <td><%= link_to 'Destroy', admin_user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New admin_user', new_admin_user_path %> | <%= link_to('Back to Menu', :controller => "admin", :action => "menu" )%>
